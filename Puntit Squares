FRECKF = input("does the father have freckles(Y,N)")
if FRECKF == "Y":
    FRECKGF1 = input("does the grandfather have freckles")
    if FRECKGF1 == "Y":
        FRECKf1 = "Y"
    elif FRECKGF1 == "N":
        FRECKf1 = "N"
    FRECKGM1 = input("does the grandmother have freckles")
    if FRECKf1 == "Y":
        if FRECKGM1 == "Y":
            FRECK1 = "YY"
        elif FRECKGM1 == "N":
            FRECK1 = "YN"
    elif FRECKf1 == "N":
        if FRECKGM1 == "Y":
            FRECK1 = "YN"
        elif FRECKGM1 == "N":
            print("error either you messed up or your adopted")
elif FRECKF == "N":
    FRECK1 = "NN"    

