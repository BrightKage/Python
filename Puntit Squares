import random
from tkinter import *

root = Tk()
canvas_width= 400
canvas_height= 400
screen = Canvas(root, width=canvas_width, height=canvas_height)
screen.pack()





HAIRTEXF = input("what type of hair does the father have(DD,Dd,dd)")
HAIRTEXM = input("what type of hair does the mother have")


if  HAIRTEXF ==  "DD" and HAIRTEXM == "DD":
    HAIRTEX= 'CURLY'
elif HAIRTEXF == "DD" and HAIRTEXM == "Dd":
    FIRST = random.randint(0,1)
    if FIRST == "0":
        HAIRTEX = 'WAVY'
    elif FIRST == "1":
        HAIRTEX = 'CURLY'
elif HAIRTEXF == "DD" and HAIRTEXM == "dd":
    HAIRTEX= 'WAVY'
elif HAIRTEXF == "Dd" and HAIRTEXM == "DD":
    FIRST = random.randint(0,1)
    if FIRST == "0":
        HAIRTEX = 'WAVY'
    elif FIRST == "1":
        HAIRTEX = 'CURLY'
elif HAIRTEXF == "Dd" and HAIRTEXM == "Dd":
    HAIRTEX= 'WAVY'
elif HAIRTEXF == "Dd" and HAIRTEXM == "dd":
    FIRST = random.randint(0,1)
    if FIRST == "0":
        HAIRTEX = 'WAVY'
    elif FIRST == "1":
        HAIRTEX = 'STRAIGHT'
elif HAIRTEXF == "dd" and HAIRTEXM == "DD":
    HAIRTEX='WAVY'
elif HAIRTEXF == "dd" and HAIRTEXM == "Dd": 
    FIRST = random.randint(0,1)
    if FIRST == "0":
        HAIRTEX = 'WAVY'
    elif FIRST == "1":
        HAIRTEX = 'STRAIGHT'
elif HAIRTEXF == "dd" and HAIRTEXM == "dd":
    HAIRTEX='STRAIGHT'

